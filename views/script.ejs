    <script type="text/javascript">
        var x = document.getElementById("myTable").rows.length - 1;
        var count = getcount(x);
        function getcount(x) {
        var count = 0;
            for (var i = 1; i <= x; i++) {
                var y;
               
                y = document.getElementById("myTable").rows[i].cells;
              
                var point = String(y[3].innerHTML).trim();
               
                

                if (point == "F" || point == "ABSENT") {
                
                    document.getElementById("myTable").rows[i].style.backgroundColor = "#ffcccc";
                    count = count + 1;
                  
                }
            }
            return count;
        }
       
        if (parseInt(count) == 0) {
            document.getElementById("message").innerHTML = "You Have Successfully Completed Your Semester with ";
            element = document.getElementById("message");
            name="green lighten-1";
            arr = element.className.split(" ");
             if (arr.indexOf(name) == -1) {
            element.className += " " + name;
               }
            
        } else if (parseInt(count) > 0) {
            document.getElementById("message").innerHTML = "Sorry You have " + count + " Backlogs";
              element = document.getElementById("message");
            name="#e53935 red darken-1";
            arr = element.className.split(" ");
             if (arr.indexOf(name) == -1) {
            element.className += " " + name;
               }
        }
        
     
    </script>
    <script type="text/javascript">
     var k = document.getElementById("myTable").rows.length - 1;

     if(k>6)
     {

   
         var sum = 0;
         for (var i = 1; i <= k; i++) {
                var p;
               
                p = document.getElementById("myTable").rows[i].cells;
              
                var grade = String(p[3].innerHTML).trim();
                sum=sum+points(grade)*p[4].innerHTML.trim();
                
                }

       var gpa=(sum/21).toPrecision(3);
               alert("Your Gpa : " + gpa);

        }

    function points(grade) {
        var point=0;
            switch (grade) {

                case "O":
                    point = 10;
                    break;
                case "S":
                    point = 9;
                    break;
                case "A":
                    point = 8;
                    break;
                case "B":
                    point = 7;
                    break;
                case "C":
                    point = 6;
                    break;
                 case "D":
                    point = 6;
                    break;
                case "F":
                    point = 0;
                    break;
                case "ABSENT":
                    point = 0;
            }
            return point;
        }
    </script>