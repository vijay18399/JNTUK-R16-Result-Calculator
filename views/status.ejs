             <section  class="row tabcontent" id="tab2" >
          
                           <div class="  col s12 m8 l8  offset-l2 offset-m2">
                              <div class="card-panel">
                                 <h4 class="header center"> Result Status</h4>
                               
                                 <form action="/status" method="post">
                                                  <div class="row">
                                       <div class="col s12">
                                          <select name="semester" class="browser-default">
                                             <option value="" disabled selected>Choose semster</option>
                                         <option   value="1-1">1-1</option>
                                         <option value="1-2">1-2</option>
                                         <option value="2-1">2-1</option>
                                         <option value="2-2">2-2</option>
                                          <option value="3-1">3-1</option>
                                           <option value="3-2">3-2</option>
                                              <option value="4-1">4-1</option>
                                           <option value="4-2">4-2</option>
                                             <option value="First year">First year</option> 
                                             <option value="Second year">Second year</option> 
                                             <option value="Third year">Third year</option> 
                                          </select>
                                       </div>
                                       <label>Select Semester</label>
                                    </div>
                                    <div class="row">
                                       <div class="col s12">
                                          <select name="Stream" class="browser-default">
                                             <option value="" disabled selected>Choose Stream</option>
                                         <option   value="B.Tech">B.Tech</option>
                                         <option value="PolyTechnic">PolyTechnic</option>
                                         <option value="Pharamacy">Pharamacy</option>
                                         <option value="Degree">Degree</option>
                                          <option value="Inter(CBSE)">Inter(CBSE)</option>
                              
                                          </select>
                                       </div>
                                       <label>Select Stream</label>
                                    </div>
                                <div class="row">
                                       <div class="col s12">
                                          <select name="Category" class="browser-default">
                                             <option value="" disabled selected>Choose Category</option>
                                         <option   value="Regular">Regular</option>
                                    
                                        
                                               <option value="supply">Supply</option>
                                          </select>
                                       </div>
                                       <label>Select Category</label>
                                    </div>
                              <div class="row">
                                       <div class="col s12">
                                          <select name="Status" class="browser-default">
                                             <option value="" disabled selected>Choose Status</option>
                                         <option   value="Phase-1 Completed">Phase-1 Completed</option>
                                         <option value="Phase-2 Completed">Phase-2 Completed</option>
                                           <option   value="Result is Out">Result is Out</option>
                                         <option value="No Information">No Information</option>
                                          </select>
                                       </div>
                                       <label>Select Status</label>
                                    </div>
                                           <div class="row">
                                       <div class="input-field col s12">
                                          <button class="btn waves-effect waves-light right" type="submit" name="action">Update
                                          <i class="material-icons right">send</i>
                                          </button>
                                       </div>
                                    </div>
                                 </form>
                               </div></div>


                                <div class="  col s12 m8 l8  offset-l2 offset-m2">
                              <div class=" card card-panel">
                             <table class="striped col s12" >
                                    <thead>
                                       <tr>
                                          <th>Semester & Stream</th>
                                          <th>Category</th>
                                          <th>Status</th>
                                          <th>Edit</th>
                                          <th>Delete</th>
                                       </tr>
                                    </thead>
                                    <tbody>
                                     <% status.forEach(function(status) { %>   
                                       <tr>
                                          <td><%= status.semester %> & <%= status.Stream %> </td>
                                          <td> <%= status.Category %> </td>
                                          <td><%= status.Status %> </td>
                                          <td> <button class="edit green waves-effect waves-light btn modal-trigger" href="#modal1" value=<%= status._id %> > <i class="material-icons ">mode_edit</i></button>
                                          </td>
                                           <td> <button class="del red waves-effect waves-light btn "  value=<%= status._id %> > <i class="material-icons ">delete</i></button>
                                          </td>
                                       </tr>
                                             <% }); %>   
                                    </tbody>
                                 </table>
                               </div>
                               </div>
  
                 </section>