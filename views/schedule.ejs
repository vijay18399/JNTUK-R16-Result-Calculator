             <section class="row tabcontent" id="tab3" >
          
                           <div class="  col s12 m8 l8  offset-l2 offset-m2">
                              <div class="card-panel">
                                 <h4 class="header center">Exam Schedules</h4>
                             
                                 <form action="/schedule" method="post" enctype="multipart/form-data">
                                 <div class="row">
                                       <div class="col s12">
                                          <select name="semester" class="browser-default">
                                             <option value="" disabled selected>Choose semster</option>
                                         <option   value="1-1">1-1</option>
                                         <option value="1-2">1-2</option>
                                         <option value="2-1">2-1</option>
                                         <option value="2-2">2-2</option>
                                          <option value="3-1">3-1</option>
                                           <option value="3-2">3-2</option>
                                              <option value="4-1">4-1</option>
                                           <option value="4-2">4-2</option>
                                             <option value="First year">First year</option> 
                                             <option value="Second year">Second year</option> 
                                             <option value="Third year">Third year</option> 
                                          </select>
                                       </div>
                                       <label>Select Semester</label>
                                    </div>
                                    <div class="row">
                                       <div class="col s12">
                                          <select name="Stream" class="browser-default">
                                             <option value="" disabled selected>Choose Stream</option>
                                         <option   value="B.Tech">B.Tech</option>
                                         <option value="PolyTechnic">PolyTechnic</option>
                                         <option value="Pharamacy">Pharamacy</option>
                                         <option value="Degree">Degree</option>
                                          <option value="Inter(CBSE)">Inter(CBSE)</option>
                              
                                          </select>
                                       </div>
                                       <label>Select Stream</label>
                                    </div>
            
                                                  <div class="row">
                                       <div class="col s12">
                                          <select name="examtype" class="browser-default">
                                             <option value="" disabled selected>Choose Exam type</option>
                                         <option   value="Mid ">Mid </option>
                                         <option value="External ">External </option>
                                                <option value="Internal ">Internal </option>
                                                <option value="Cocubes ">Cocubes </option>
                                          </select>
                                       </div>
                                       <label>Select Exam type</label>
                                    </div>
                           
                                    <div class="row">
                                       <div class="input-field col s12">
                                          <i class="material-icons prefix">file_upload</i>
                                          <input id="attachment" type="file" name="attachment" class="validate">
                                       </div>
                                    </div>
                                    <div class="row">
                                       <div class="input-field col s12">
                                          <button class="btn waves-effect waves-light right" type="submit" name="action">Upload
                                          </button>
                                       </div>
                                    </div>
                                 </form>
                               </div></div>
                                         <div class="  col s12 m8 l8  offset-l2 offset-m2">
                              <div class=" card card-panel">
                             <table class="striped col s12" >
                                    <thead>
                                       <tr>
                                          <th>Semester & </th>
                                          <th>Stream</th>
                                          <th>exam type</th>
                                          <th>file</th>
                                          <th>Delete</th>
                                       </tr>
                                    </thead>
                                    <tbody>
                                     <% schedule.forEach(function(schedule) { %>   
                                       <tr>
                                          <td><%= schedule.semester %>  </td>
                                          <td> <%= schedule.Stream %> </td>
                                          <td><%= schedule.examtype %> </td>
                                     
                                         <td><img class="materialboxed"  width="90"   src="<%= schedule.file %>" height="50px" > </td>
                                           <td> <button class="delsch red waves-effect waves-light btn "  value=<%= schedule._id %> > <i class="material-icons ">delete</i></button>
                                          </td>
                                       </tr>
                                             <% }); %>   
                                    </tbody>
                                 </table>
                               </div>
                               </div>
  
                 </section>