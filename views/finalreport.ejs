<!DOCTYPE html>
<html lang="en">

      <% include head %>
  <body class="layout-light">
    <!-- Start Page Loading 
    <div id="loader-wrapper">
      <div id="loader"></div>
      <div class="loader-section section-left"></div>
      <div class="loader-section section-right"></div>
    </div>
    <!-- End Page Loading -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- START HEADER -->
    <header id="header" class="page-topbar">
      <!-- start header nav-->
      <div class="navbar-fixed">
        <nav class="navbar-color">
          <div class="nav-wrapper">
            <ul>
              <li >
                <h1 class="logo-wrapper ">
             <a  class=" center brand-logo darken-1">
                    <img src="https://www.vanhool.be/assets/images/logo-autocars-ex.png"  alt="materialize logo">
                  <span class="hide-on-med-and-down blue-text"> <span class="grey-text">Exam</span> Portal</span>
                  </a>
                </h1>
              </li>
            </ul>

          </div>
        </nav>
      </div>
      <!-- end header nav-->
    </header>
    <!-- END HEADER -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- START MAIN -->
    <main class="row" >
   <div class="gradient-45deg-indigo-light-blue padding-1  white-text center" ><h5>Generated Report</h5></div>
         <div class="card col l10 offset-l1">

</div>
      <!-- END WRAPPER -->
</main>


                   <div >
                  <div class="  col s12 m8 l8  offset-l2 offset-m2">
                              <div class=" card card-panel">
                  
                         <% report.forEach(function(report) { %>  
                      
                        <h3><%=JSON.stringify( report._id) %></h3><h5>(<%=JSON.stringify( report.count) %>)</h5>
                        <div class="row"> 
<% for(var i=0; i < report.studentids.length; i++) { %>
  <div class="col s12 l2">
     <span><%= report.studentids[i].Htno %></span>
      <span>(<%= report.studentids[i].Grade %>)</span>
   </div>
<% } %>

</div>
                    
                       
                    
                       
                       <% }); %>  
                  
                </div>
              </div>
                </div>
      
  </body>
</html>