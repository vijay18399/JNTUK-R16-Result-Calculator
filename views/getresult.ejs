<!DOCTYPE html>
<html lang="en">

      <% include head %>
  <body class="layout-light">
    <!-- Start Page Loading 
    <div id="loader-wrapper">
      <div id="loader"></div>
      <div class="loader-section section-left"></div>
      <div class="loader-section section-right"></div>
    </div>
    <!-- End Page Loading -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- START HEADER -->
    <header id="header" class="page-topbar">
      <!-- start header nav-->
      <div class="navbar-fixed">
        <nav class="navbar-color">
          <div class="nav-wrapper">
            <ul>
              <li >
                <h1 class="logo-wrapper ">
                  <a  class=" center brand-logo darken-1">
                    <img src="http://web.aditya.ac.in/cbse/wp-content/uploads/2017/04/logo-SMALL.png"  alt="materialize logo">
                    <span class="center logo-text hide-on-med-and-down">Aditya Results</span>
                  </a>
                </h1>
              </li>
            </ul>

          </div>
        </nav>
      </div>
      <!-- end header nav-->
    </header>
    <!-- END HEADER -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- START MAIN -->
    <main>

          <div class="row">
                <div class="col s12 m12 offset-l2 l8">
                  <div class="card-panel">
                    <h4 class="header2 center">Get Result </h4>
                    <div class="row">
                      <form class="col s12" action="/myresult" method="post">
                              <div class="row ">
                          <div class="input-field col s12">
                            <input value="<%= Semester %>" name="Semester" type="text">
                            <label for="first_name">Semester</label>
                          </div>
                        </div>
                        <div class="row">
                          <div class="input-field col s12">
                            <input name="ht_no" type="text">
                            <label for="first_name">Roll Number</label>
                          </div>
                        </div>

                          <div class="row center">
                            <div class="input-field col s12">
                              <button class="waves-effect getresult waves-light btn"  type="submit" name="action">Get Result
                               
                              </button>
                            </div>
                          </div>
                        </div>
                      </form>
                    </div>
                  </div>
                </div>
          <div class="divider"></div> 
            <!-- Scaled in -->


      <!-- END WRAPPER -->
</main>


    <!-- END MAIN -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- START FOOTER -->
  
    <!-- END FOOTER -->
      <% include script %>
  </body>
</html>