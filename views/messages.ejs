<section class="row tabcontent" id="tab6" >

                               <table class="striped " >
                                    <thead>
                                       <tr>
                                          <th>Name</th>
                                          <th>Email</th>
                                          <th>Message</th>
                                          <th>Delete</th>
                                       </tr>
                                    </thead>
                                    <tbody>
                                        <% messages.forEach(function(messages) { %>   
                                       <tr>
                                          <td><%= messages.name %></td>
                                      <td><%= messages.email %></td>
                                      <td><%= messages.message %></td>
                                          <td>  <button class="btn small deletem red" value=<%= messages._id %> type="submit" name="action">
                                             <i class="material-icons ">delete</i>
                                             </button>
                                          </td>
                                       </tr>
                                        <% }); %>  
                                    </tbody>
                                 </table>
                             
                           </section>